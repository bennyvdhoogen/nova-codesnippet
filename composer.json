{
    "name": "bennyvdhoogen/nova-codesnippet",
    "description": "A Laravel Nova Code Snippet field.",
    "keywords": [
        "laravel",
        "nova",
        "code snippet",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "bennyvdhoogen\\Codesnippet\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "bennyvdhoogen\\Codesnippet\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
